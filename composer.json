{
  "name": "floweye/client",
  "description": "Floweye PHP API client",
  "keywords": [
    "floweye",
    "api",
    "sdk",
    "http"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/flowsource/floweye-client",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.1.0",
    "nette/utils": "^2.2 || ^3.0 || ^4.0"
  },
  "require-dev": {
    "nette/di": "^2.4.15 || ^3.0.0",
    "ninjify/coding-standard": "^0.12.0",
    "phpstan/phpstan": "^0.12.0",
    "phpstan/phpstan-deprecation-rules": "^0.12.0",
    "phpstan/phpstan-nette": "^0.12.0",
    "phpstan/phpstan-strict-rules": "^0.12.0",
    "phpunit/phpunit": "^8.5.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Floweye\\Client\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Floweye\\Client\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
